{
  "functions": {
    "source": "./dist/functions",
    "predeploy": [
      "npm run typecheck-app",
      "npm run build-functions",
      "npm run build-app",
      "npm run copy-deps"
    ]
  },
  "hosting": {
    "public": "./dist/public",
    "rewrites": [
      {
        "source": "**/**",
        "function": "nextApp"
      }
    ],
    "predeploy": "npm run build-public"
  }
}
